@model AspNetUserRolesEntityFramework.Models.MachineLearningCompaniesFeedback

@{
    ViewData["Title"] = "Details";
}
    <div style="margin-left:20px; margin-top: 10px;">

        <h1>Details</h1>

        <div>
            <h4>Feedback on Machine Learning Companies</h4>
            <div style="margin-left:60px; margin-top: 10px;">
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PostDate)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PostDate)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.UserName)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.UserName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.TopicTitle)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.TopicTitle)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.StarRating)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.StarRating)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.MessageContent)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.MessageContent)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Like)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Like)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Dislike)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Dislike)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Company)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Company)
                    </dd>
                </dl>
            </div>

            <div>
                <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
                <a asp-action="Index">Back to Machine Learning Page</a>
            </div>

        </div>
    </div>